#!/bin/bash

# =============================
# DISABLED
# =============================

if [[ -f "./.disabled.flag" ]]
then
  echo "./.disabled.flag set. delete to continue"
  exit 1
fi

# =============================
# READ ENV
# =============================

if [[ ! -f "./env" ]]
then
  echo "./env file missing"
  exit 1
fi

exit 1

